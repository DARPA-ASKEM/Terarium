{
	"name": "hmi-client",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite --host",
		"build": "vue-tsc --noEmit && vite build",
		"typecheck": "yarn workspace graph-scaffolder tsc --build && vue-tsc --noEmit",
		"preview": "vite preview",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:snapshots": "playwright test --update-snapshots",
		"test:ct": "playwright test -c playwright-ct.config.ts"
	},
	"dependencies": {
		"@carbon/icons-vue": "10.60.0",
		"@ibm/plex": "6.1.1",
		"@uncharted.software/design-tokens": "1.1.0",
		"@uncharted.software/facets-core": "3.2.6",
		"@uncharted.software/facets-plugins": "3.2.5",
		"axios": "1.1.3",
		"chroma-js": "2.4.2",
		"d3": "7.6.1",
		"dagre": "0.8.5",
		"eslint-import-resolver-typescript": "3.5.2",
		"graph-scaffolder": "*",
		"lodash": "4.17.21",
		"pinia": "2.0.28",
		"pixi-viewport": "4.37.0",
		"pixi.js": "7.0.5",
		"primeicons": "6.0.1",
		"primevue": "3.21.0",
		"sass": "1.56.1",
		"vue": "3.2.45",
		"vue-router": "4.1.6"
	},
	"devDependencies": {
		"@pinia/testing": "0.0.14",
		"@playwright/experimental-ct-vue": "1.27.1",
		"@playwright/test": "1.27.1",
		"@rushstack/eslint-patch": "1.2.0",
		"@types/d3": "7.4.0",
		"@types/lodash": "4.14.191",
		"@types/node": "18.11.18",
		"@vitejs/plugin-vue": "3.2.0",
		"@vitest/coverage-c8": "0.24.5",
		"typescript": "4.8.4",
		"vite": "3.2.5",
		"vitest": "0.24.5",
		"vue-tsc": "1.0.22"
	}
}
